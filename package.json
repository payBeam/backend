{
  "name": "backend",
  "version": "1.0.0",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "npm run db:init && nodemon -r module-alias/register -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "db:init": "cd ./src && npx prisma generate",
    "start": "node dist/server.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build"
  },
  "_moduleAliases": {
    "@": "./src"
  },
  "keywords": [
    "crypto",
    "payment"
  ],
  "author": "Dunsin Abisuwa",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "module-alias": "^2.2.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.10",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
